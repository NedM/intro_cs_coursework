package client;

public class ABSBoardSquare
{
	
	private final int row;
	private final int col;
	
	public ABSBoardSquare ( int row, int col )
	{
		this.row = row;
		this.col = col;
	}
	
	public int getRow()
	{
		return this.row;
	}
	
	public int getColumn()
	{
		return this.col;
	}
	
	@Override
	public String toString()
	{
		return "row: " + this.getRow() + ", column: " + this.getColumn();
	}
	
}
