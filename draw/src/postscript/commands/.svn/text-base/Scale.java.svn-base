package postscript.commands;

/**
 * Represents the postscript command `x y scale'.
 */
public class Scale implements PostscriptCommand {
	
	private double x;
	private double y;

	public Scale(double x, double y) {
		this.x = x;
		this.y = y;
	}

	public String toString() {
		return Double.toString(x) + " " + Double.toString(y) + " " + "scale\n";
	}
}
